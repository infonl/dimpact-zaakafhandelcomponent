    "@angular/elements": "19.2.15",
    "@angular/forms": "19.2.15",
    "@angular/material": "19.2.19",
    "@angular/material-moment-adapter": "19.2.19",
    "@angular/platform-browser": "19.2.15",
    "@angular/platform-browser-dynamic": "19.2.15",
    "@angular/router": "19.2.15",
    "@formio/angular": "9.0.1",
    "@ngx-translate/core": "17.0.0",
    "@ngx-translate/http-loader": "17.0.0",
    "@tanstack/angular-query-experimental": "5.90.12",
    "@tanstack/query-async-storage-persister": "5.90.12",
    "@tanstack/query-persist-client-core": "5.91.9",
    "bootstrap": "4.6.2",
    "moment": "2.30.1",
    "ngx-editor": "18.0.0",
    "ol": "10.7.0",
    "openapi-fetch": "0.15.0",
    "proj4": "2.20.1",
    "rxjs": "7.8.2",
    "ts-expect": "1.3.0",
    "ts-pattern": "5.9.0",
