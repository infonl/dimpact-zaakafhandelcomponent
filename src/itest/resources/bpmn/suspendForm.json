{
  "display": "form",
  "name": "suspendForm",
  "title": "Suspend form",
  "type": "form",
  "components": [
    {
      "components": [
        {
          "label": "Suspend days",
          "applyMaskOn": "change",
          "mask": false,
          "tableView": false,
          "persistent": false,
          "delimiter": false,
          "requireDecimal": false,
          "inputFormat": "plain",
          "truncateMultipleSpaces": false,
          "validate": {
            "min": 0,
            "max": 90
          },
          "validateWhenHidden": false,
          "key": "suspendDays",
          "type": "number",
          "input": true,
          "defaultValue": 10
        }
      ],
      "input": false,
      "tableView": false
    },
    {
      "type": "button",
      "label": "Suspend",
      "key": "submit",
      "disableOnInvalid": true,
      "input": true,
      "tableView": false
    }
  ]
}
